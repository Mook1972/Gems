    <DataSet Name="subscriptionMail">
      <Fields>
        <Field Name="subscriptionID">
          <DataField>subscriptionID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="itemID">
          <DataField>itemID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="path">
          <DataField>path</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="statusInd">
          <DataField>statusInd</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="lastStatus">
          <DataField>lastStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lastRunTime">
          <DataField>lastRunTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="eventType">
          <DataField>eventType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="deliveryExtension">
          <DataField>deliveryExtension</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="modifiedDate">
          <DataField>modifiedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ToEmail">
          <DataField>ToEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CCEmail">
          <DataField>CCEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BCCEmail">
          <DataField>BCCEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IncludeReport">
          <DataField>IncludeReport</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RenderFormat">
          <DataField>RenderFormat</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IncludeLink">
          <DataField>IncludeLink</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Comment">
          <DataField>Comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ds_ReportServer</DataSourceName>
        <CommandText>Set transaction isolation level read uncommitted 
select		Subscriptions.subscriptionID,
                                Catalog.itemID,
		Subscriptions.Description AS [description], 
		Catalog.Name AS [name], 
		Catalog.Path AS [path], 
                                case left(Subscriptions.lastStatus,7) when 'Error: ' then 0 when 'failure' then 0 else 1 end as statusInd,
		Subscriptions.lastStatus,
		Subscriptions.lastRunTime,
		Subscriptions.eventType,
		Subscriptions.deliveryExtension,
		Subscriptions.modifiedDate,

		SUBSTRING(ExtensionSettings, 
			LEN('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', ExtensionSettings, 
			CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - 
			(LEN('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings))) 
		AS ToEmail,

		CASE CHARINDEX('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN ''
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', ExtensionSettings, 
			CHARINDEX('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - 
			(LEN('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;CC&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings))) END 
		AS CCEmail,

		CASE CHARINDEX('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN ''
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings), CHARINDEX('&lt;/Value&gt;', ExtensionSettings, CHARINDEX('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings) + 1) - (LEN('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;BCC&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings))) END 
		AS BCCEmail,

		SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;IncludeReport&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;IncludeReport&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', ExtensionSettings, CHARINDEX('&lt;Name&gt;IncludeReport&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings) + 1) - (LEN('&lt;Name&gt;IncludeReport&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;IncludeReport&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) 
		AS IncludeReport,

		SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;RenderFormat&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;RenderFormat&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', ExtensionSettings, CHARINDEX('&lt;Name&gt;RenderFormat&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings) + 1) - (LEN('&lt;Name&gt;RenderFormat&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;RenderFormat&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) 
		AS RenderFormat,

		SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;IncludeLink&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;IncludeLink&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', ExtensionSettings, CHARINDEX('&lt;Name&gt;IncludeLink&lt;/Name&gt;&lt;Value&gt;', 
			ExtensionSettings) + 1) - (LEN('&lt;Name&gt;IncludeLink&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;IncludeLink&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) 
		AS IncludeLink,

		CASE CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN ''
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), CHARINDEX('&lt;/Value&gt;', 
			ExtensionSettings, CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - 
			(LEN('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) END 
		AS Comment

FROM                      ReportSchedule 
		INNER JOIN Schedule ON ReportSchedule.ScheduleID = Schedule.ScheduleID 
		INNER JOIN Subscriptions ON ReportSchedule.SubscriptionID = Subscriptions.SubscriptionID 
		INNER JOIN [Catalog] ON ReportSchedule.ReportID = [Catalog].ItemID AND Subscriptions.Report_OID = [Catalog].ItemID
WHERE		Subscriptions.DeliveryExtension = 'Report Server Email'
                                and (
                               @reportID = 'NOID' or @reportID = convert(varchar(500),Catalog.itemID)
                               )</CommandText>
        <QueryParameters>
          <QueryParameter Name="@reportID">
            <Value>=Parameters!reportID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
