        </Field>
        <Field Name="Comment">
          <DataField>Comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ds_ReportServer</DataSourceName>
        <CommandText>Set transaction isolation level read uncommitted 
select		Subscriptions.subscriptionID,
                                Catalog.itemID,
		Subscriptions.Description AS [description], 
		Catalog.Name AS [name], 
		Catalog.Path AS [path], 
                                case left(Subscriptions.lastStatus,7) when 'Error: ' then 0 when 'failure' then 0 else 1 end as statusInd,
		Subscriptions.lastStatus,
		Subscriptions.lastRunTime,
		Subscriptions.eventType,
		Subscriptions.deliveryExtension,
		Subscriptions.modifiedDate,

                                SUBSTRING(Subscriptions.ExtensionSettings, LEN('&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;') + 
	        	                CHARINDEX('&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings), CHARINDEX('&lt;/Value&gt;', 
			Subscriptions.ExtensionSettings, CHARINDEX('&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings) + 1) 
			- (LEN('&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings)))
		AS PATH, 

		CASE CHARINDEX('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN '' 
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), CHARINDEX('&lt;/Value&gt;', ExtensionSettings, 
			CHARINDEX('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - (LEN('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;FILENAME&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) END 
		AS FILENAME, 

                                CASE CHARINDEX('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN '' 
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), CHARINDEX('&lt;/Value&gt;', 
			ExtensionSettings, CHARINDEX('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - 
			(LEN('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;FILEEXTN&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) END 
		AS FILEEXTN, 

		SUBSTRING(Subscriptions.ExtensionSettings, LEN('&lt;Name&gt;RENDER_FORMAT&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;RENDER_FORMAT&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings), 
			CHARINDEX('&lt;/Value&gt;', Subscriptions.ExtensionSettings, CHARINDEX('&lt;Name&gt;RENDER_FORMAT&lt;/Name&gt;&lt;Value&gt;', 
			Subscriptions.ExtensionSettings) + 1) - (LEN('&lt;Name&gt;RENDER_FORMAT&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;RENDER_FORMAT&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings))) 
		AS RENDER_FORMAT, 

		SUBSTRING(Subscriptions.ExtensionSettings, LEN('&lt;Name&gt;WRITEMODE&lt;/Name&gt;&lt;Value&gt;') 
			+ CHARINDEX('&lt;Name&gt;WRITEMODE&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings), CHARINDEX('&lt;/Value&gt;', 
			Subscriptions.ExtensionSettings, CHARINDEX('&lt;Name&gt;WRITEMODE&lt;/Name&gt;&lt;Value&gt;', 
			Subscriptions.ExtensionSettings) + 1) - (LEN('&lt;Name&gt;WRITEMODE&lt;/Name&gt;&lt;Value&gt;') 
                                               + CHARINDEX('&lt;Name&gt;WRITEMODE&lt;/Name&gt;&lt;Value&gt;', Subscriptions.ExtensionSettings))) 
		AS WRITEMODE, 

		CASE CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) 
			WHEN 0 THEN '' 
			ELSE SUBSTRING(ExtensionSettings, LEN('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;') + 
			CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings), CHARINDEX('&lt;/Value&gt;', 
			ExtensionSettings, CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings) + 1) - 
			(LEN('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;') + CHARINDEX('&lt;Name&gt;Comment&lt;/Name&gt;&lt;Value&gt;', ExtensionSettings))) END 
		AS Comment

FROM		ReportSchedule 
		INNER JOIN Schedule ON ReportSchedule.ScheduleID = Schedule.ScheduleID 
		INNER JOIN Subscriptions ON ReportSchedule.SubscriptionID = Subscriptions.SubscriptionID 
		INNER JOIN Catalog ON ReportSchedule.ReportID = Catalog.ItemID AND Subscriptions.Report_OID = Catalog.ItemID
WHERE                   (Subscriptions.DeliveryExtension = 'Report Server FileShare')
