          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="itemID">
          <DataField>itemID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="parentID">
          <DataField>parentID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ds_ReportServer</DataSourceName>
        <CommandText>Set transaction isolation level read uncommitted 
select		case	when	@searchBy = 'User' then u.UserName
			when	@searchBy = 'Object' then c.name
			else	case when c.parentID is null then 'Home' else c.name end end as searchedObject,
		u.userName,
                                u.userID,
		r.roleName,
		r.description,
		case when c.parentID is null then 'Home' else c.name end as name,
		case when c.parentID is null then '/Home' else c.path end as path,
		c.itemID,
		c.parentID,
		c.type			 

from		policyUserRole pur
		inner join roles r on pur.roleID = r.roleID
		inner join users u on pur.userID = u.userID
		inner join catalog c on pur.policyID = c.policyID

where		type in (1,2)
		and (
			(@searchBy = 'ALL')
			or
			(@searchBy = 'User' and (@searchVal = convert(varchar(500),u.userID)) or (u.userName='Everyone') )
			or
			(@searchBy = 'Object' and @searchVal = convert(varchar(500),c.itemID))
		)

order by	               searchedObject</CommandText>
        <QueryParameters>
          <QueryParameter Name="@searchBy">
            <Value>=Parameters!searchBy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@searchVal">
            <Value>=Parameters!searchVal.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataLocation">
      <Fields>
        <Field Name="dataLocation">
          <DataField>dataLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ds_ReportServer</DataSourceName>
        <CommandText>Set transaction isolation level read uncommitted 

select top 1 @@serverName + '\' + table_Catalog as dataLocation from information_schema.tables</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
