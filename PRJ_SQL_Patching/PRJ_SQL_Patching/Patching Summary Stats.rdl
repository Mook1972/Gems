  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SummaryStats</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PatchedStatus">
            <Value>=Parameters!PatchedStatus.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>---SELECT        ServerType, Edition, TotalInstances, PatchedStatus
---FROM            SummaryStats
---WHERE        (PatchedStatus IN (@PatchedStatus))

  SELECT        Case When Edition = 'Could Not Connect' then 1
				When Edition = 'SQL Server 2008' then 2
				When Edition = 'SQL Server 2008 R2' then 3
				When Edition = 'SQL Server 2012' then 4
				When Edition = 'SQL Server 2014' then 5
				When Edition = 'SQL Server 2016' then 6
				When Edition = 'SQL Server 2017' then 7
					else 8 end as versort
				,ServerType, Edition, TotalInstances, PatchedStatus
FROM           SummaryStats 
WHERE        (PatchedStatus IN (@PatchedStatus))</CommandText>
      </Query>
      <Fields>
        <Field Name="versort">
          <DataField>versort</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServerType">
          <DataField>ServerType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Edition">
          <DataField>Edition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalInstances">
          <DataField>TotalInstances</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PatchedStatus">
          <DataField>PatchedStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="gTotInstances">
      <Query>
        <DataSourceName>SummaryStats</DataSourceName>
        <CommandText>SELECT        SUM(TotalInstances) AS GlobalSumINST
FROM            SummaryStats</CommandText>
      </Query>
      <Fields>
        <Field Name="GlobalSumINST">
          <DataField>GlobalSumINST</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="totMissingApps">
      <Query>
        <DataSourceName>SummaryStats</DataSourceName>
        <CommandText>SELECT        COUNT(0) AS APP_Unknown
FROM            SQL_Estate_Patched_Status
WHERE        (APPL_name = 'NULL') AND (PatchedStatus &lt;&gt; 'Patched')</CommandText>
      </Query>
      <Fields>
        <Field Name="APP_Unknown">
          <DataField>APP_Unknown</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
